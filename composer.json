{
  "name": "hjun-tech/think-captcha-restapi",
  "description": "captcha package for thinkphp",
  "authors": [{
    "name": "hjun-tech",
    "email": "open-wx@hjun-tech.com"
  }],
  "license": "Apache-2.0",
  "require": {
    "topthink/framework": "^6.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "think\\captcha\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "think": {
      "services": [
        "think\\captcha\\CaptchaService"
      ],
      "config": {
        "captcha": "src/config.php"
      }
    }
  }
}
